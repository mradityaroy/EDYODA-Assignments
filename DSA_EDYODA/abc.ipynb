     "text": [
      "[1, 2, 3, 4]\n",
      "Reversed list is\n",
      "[4, 3, 2, 1]\n"
     ]
    }
   ],
   "source": [
    "A = [1, 2, 3, 4]\n",
    "print(A)\n",
    "n=len(A)-1\n",
    "reverseList(A, 0, n)\n",
    "print(\"Reversed list is\")\n",
    "print(A)"
   ]
  }
