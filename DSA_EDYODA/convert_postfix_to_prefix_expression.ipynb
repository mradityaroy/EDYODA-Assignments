    "s = \"*-A/BC-/AKL\"\n",
    "stack = []\n",
    "\n",
    "operators = set(['+', '-', '*', '/', '^'])\n",
    "\n",
    "s = s[::-1]\n",
    "\n",
    "for i in s:\n",
    "\n",
    "\tif i in operators:\n",
    "\n",
    "\t\ta = stack.pop()\n",
    "\t\tb = stack.pop()\n",
    "\n",
    "\t\ttemp = a+b+i\n",
    "\t\tstack.append(temp)\n",
    "\n",
    "\telse:\n",
    "\t\tstack.append(i)\n",
    "\n",
    "print(*stack)\n"
   ]
