    "Count_Even = 0\r\n",
    "Count_Odd = 0\r\n",
    "for i in range(1, 10):\r\n",
    "    if i%2 == 0:\r\n",
    "        Count_Even += 1\r\n",
    "    else:\r\n",
    "        Count_Odd += 1\r\n",
    "print(\"Number of even numbers : \",Count_Even)\r\n",
    "print(\"Number of odd numbers : \",Count_Odd)"
   ],
